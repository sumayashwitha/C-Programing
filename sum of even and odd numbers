#include<stdio.h>
int main(void){
   int num, i,sum=0;
  printf("enter the number:");
  scanf("%d",&num);
  for(i=1;i<=num;i++){
       if(i%2==0){
          sum=sum+i;      
      }
  }
  printf("the sum even numbers between 1 and %d=%d\n", num,sum);
  for(i=1'i<=num;i++){
        if(i%2!=0){
           sum=sum+i;
        }
   }
   printf("the sum of odd numbers between 1 and %d =%d\n", num,sum);
  return 0;
}

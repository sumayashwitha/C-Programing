#include<stdio.h>
int main(void){
    int a,b,c;
    int max, min;
    printf("enter the three integers:");  
    scanf("%d %d %d", &a, &b,&c);
    max=a;
    min=a;
    if(b>max){
        max=b;
    }if(c>max){
        max=c;
    }
    if(b<min){
        min=b;
    }if(c<min){
        min=c;
    }
    printf(" the maximum number=%d\n", max);
    printf("the minimum number=%d\n", min);
  return 0;
}

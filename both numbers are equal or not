#include<stdio.h>
int main(void){
     int num1, num2;
     printf("enter the first number:");
     scanf("%d", &num1);
     printf("enter the second number:");
     scanf("%d", &num2);
     if(num1==num2){
          printf("both the numbers are equal");
     }else{
          printf("both numbers are not equal);
     }
    return 0;
}
